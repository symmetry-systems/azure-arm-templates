{
    "type": "Microsoft.Resources/deploymentScripts",
    "apiVersion": "2020-10-01",
    "name": "runPowerShellInline",
    "location": "[resourceGroup().location]",
    "kind": "AzurePowerShell",
    "properties": {
        "arguments": "-name \\\"DG\\\"",
        "azPowerShellVersion": "6.4",
        "cleanupPreference": "OnSuccess",
        "containerSettings": {
            "containerGroupName": "customaci"
        },
        // "environmentVariables": [
        //     {
        //         "name": "string",
        //         "secureValue": "string",
        //         "value": "string"
        //     }
        // ],
        "forceUpdateTag": "1",
        "primaryScriptUri": "https://raw.githubusercontent.com/symmetry-systems/azure-arm-templates/app-reg/scripts/list-subscriptions.ps1",
        "retentionInterval": "P1D",
        "storageAccountSettings": {
            "storageAccountName": "tempStorageAccount",
            "storageAccountKey": "myKey"
        },
        "timeout": "PT30M"
    }
}